{
  "manifest_version": 2,
  "name": "Sweep",
  "description": "Closes long-time unused tabs and adds them to bookmarks.",
  "version": "1.0",
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "bookmarks"
  ],
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": true
  },
  "options_page": "options/options.html",
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_popup": "popup/popup.html"
  }
}
